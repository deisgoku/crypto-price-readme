{
  "version": 2,
  "redirects": [
    {
      "source": "/",
      "destination": "https://crypto-price-on.vercel.app/unlock?ref=github",
      "permanent": true
    }
  ],
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/api/bot",
      "dest": "/api/bot.js"
    }
  ]
}
